{
  "name": "ItaloTurkBook",
  "description": "Simple automated italo book for carnet using puppeteer",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/CatMonster/ItaloTurkBook",
  "author": "CatMonster",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --exec node_modules/.bin/babel-node src/index.js -r dotenv/config",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src --ext js",
    "prepare": "husky install"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "puppeteer": "^11.0.0",
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "nodemon": "^2.0.15",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run pretty-quick --staged && yarn lint"
    }
  }
}
