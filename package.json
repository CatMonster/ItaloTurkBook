{
  "name": "ItaloTurkBook",
  "description": "Simple automated italo book for carnet using puppeteer",
  "version": "1.0.0",
  "main": "./src/index.js",
  "repository": "https://github.com/CatMonster/ItaloTurkBook",
  "author": "CatMonster",
  "license": "MIT",
  "scripts": {
    "start": "nodemon ./src/index.js --exec babel-node --presets babel-preset-env",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src --ext js",
    "prepare": "husky install"
  },
  "dependencies": {
    "command-line-usage": "^6.1.1",
    "dotenv": "^10.0.0",
    "puppeteer": "^11.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "command-line-args": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run pretty-quick --staged && yarn lint"
    }
  }
}
